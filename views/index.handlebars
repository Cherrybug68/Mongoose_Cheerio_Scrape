<div class="row text-center">
    <h1>Note Scraper</h1>
    <h2><i>Say something funny here.</i></h2>
    <button id = "scrape" class="btn btn-normal">Click to get news article headlines</button>
    {{!--<div id="articles"></div>--}}
    {{!--<div id="notes"></div>--}}
</div>

    <div class="row">
        <div class="col-sm-12">
            <div class="panel panel-success">
                <div class="panel-heading">
                    <h3 class="panel-title">Articles</h3>
                </div>
                <div class="panel-body">
                     <ul>
                        {{#each this.articles}}
                          <li>
                              <p><a href="{{this.link}}">{{this.title}}</a></p>
                         </li>
                        {{/each}}
                    </ul>
                </div>
            </div>
        </div>
    </div>